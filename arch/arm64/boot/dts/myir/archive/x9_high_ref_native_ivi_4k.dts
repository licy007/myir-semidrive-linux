	sdriv,crtc = <&crtc0>;
	status = "okay";
};
&dp1 {
	status = "okay";
};
&dp2 {
	status = "okay";
};
&dc1 {
	status = "okay";
};
&dc2 {
	status = "okay";
};
&crtc0 {
	dpc-master = <&dp1 &dc1>;
	dpc-slave = <&dp2 &dc2>;
	display-mode = <3>;
	status = "okay";

