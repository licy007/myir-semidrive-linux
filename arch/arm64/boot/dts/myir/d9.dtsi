/ {
	compatible = "semidrive,kunlun";
	#address-cells = <2>;
	#size-cells = <2>;

	psci: psci {
		compatible	= "arm,psci-1.0";
		method		= "smc";
		cpu_on		= <0xc4000003>;
		cpu_off		= <0x84000002>;
		cpu_suspend	= <0xc4000001>;
	};

	soc {
		compatible = "simple-bus";
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		smmu: smmu@35800000 {
			compatible = "arm,smmu-v3";
			reg = <0x0 0x35800000 0x0 0x800000>;
			interrupts = <0 277 IRQ_TYPE_EDGE_RISING>,
					<0 285 IRQ_TYPE_EDGE_RISING>,
					<0 278 IRQ_TYPE_EDGE_RISING>,
					<0 280 IRQ_TYPE_EDGE_RISING>;
			interrupt-names = "eventq", "priq", "cmdq-sync", "gerror";
			#iommu-cells = <1>;
			status = "disabled";
		};

		uart1: serial@300f0000 {
			compatible = "snps,dw-apb-uart";
			reg = <0 0x300f0000 0 0x100>;
			reg-shift=<2>;
			reg-io-width=<4>;
			interrupts = <0 16 4>;
			clock-frequency=<80000000>;
/*			clocks = <&UART_SEC1_10>;*/
			clock-names = "baudclk";
			status = "disabled";
		};

		uart2: serial@30100000 {
