					cpu_trip1:t1{
						temperature = <110000>;
						type = "passive";
						hysteresis = <0>;
					};
					cpu_trip2:t2{
						temperature = <120000>;
						type = "passive";
						hysteresis = <0>;
					};
					crt_trip0:t4{
						temperature = <125000>;
						type = "critical";
						hysteresis = <0>;
					};
				};
				cooling-maps {
					bind0 {
						contribution = <0>;
						trip = <&cpu_trip0>;
						cooling-device = <&cpufreq_cluster0 THERMAL_NO_LIMIT 2>;
					};
					bind1{
						contribution = <0>;
						trip = <&cpu_trip1>;
						cooling-device = <&cpufreq_cluster0 THERMAL_NO_LIMIT 3>;
					};
					bind2{
						contribution = <0>;
						trip = <&cpu_trip2>;
						cooling-device = <&cpucore_cluster0 THERMAL_NO_LIMIT 3>;
					};
				};
			};
		};

		rfkill: rfkill@0{
			compatible = "semidrive,sd-rfkill";
			status = "disabled";
		};

		sdrfkill: rfkill@1{
			compatible = "semidrive,sd-rfkill-v2";
			bluetooth-name="bt";
			bluetooth-reset-gpio=<0 0 0>;
			bluetooth-en-gpio=<0 0 0>;
			bluetooth-delay=<10>;
			wlan-name="wlan";
			wlan-reset-gpio=<0 0 0>;
			wlan-en-gpio=<0 0 0>;
			wlan-delay=<10>;
			gps-name="gps";
			gps-reset-gpio=<0 0 0>;
			gps-en-gpio=<0 0 0>;
			gps-delay=<10>;
			status = "disabled";
		};

		flexcan1: can@30030000 {
			compatible = "semidrive,flexcan";
			reg = <0x0 0x30030000 0x0 0x10000>;
			interrupts = <GIC_SPI 118 IRQ_TYPE_LEVEL_HIGH>;
			status = "disabled";
		};

		flexcan2: can@30040000 {
			compatible = "semidrive,flexcan";
			reg = <0x0 0x30040000 0x0 0x10000>;
			interrupts = <GIC_SPI 119 IRQ_TYPE_LEVEL_HIGH>;
			status = "disabled";
		};

		flexcan3: can@30050000 {
			compatible = "semidrive,flexcan";
			reg = <0x0 0x30050000 0x0 0x10000>;
			interrupts = <GIC_SPI 120 IRQ_TYPE_LEVEL_HIGH>;
			status = "disabled";
		};

		flexcan4: can@30060000 {
			compatible = "semidrive,flexcan";
			reg = <0x0 0x30060000 0x0 0x10000>;
			interrupts = <GIC_SPI 121 IRQ_TYPE_LEVEL_HIGH>;
			status = "disabled";
		};

		pfm: pfm@0x33020000 {
				compatible = "semidrive,pfm";
				reg = <0x0 0x33020000 0x0 0x10000>,
					  <0x0 0x45e00000 0x0 0x2000>;
				reg-names = "ctrl_reg", "ram_base";
				interrupts = <0 186 4>;
				time-window-mode = <1>;
				sdrv,group_info =
				<SDRV_PFM_GROUP_INFO_AP1>,
				<SDRV_PFM_GROUP_INFO_VDSP>,
				<SDRV_PFM_GROUP_INFO_CSI1_CSI3>,
				<SDRV_PFM_GROUP_INFO_DC1>,
				<SDRV_PFM_GROUP_INFO_DP1>,
				<SDRV_PFM_GROUP_INFO_GPU1>,
				<SDRV_PFM_GROUP_INFO_MJPEG>,
				<SDRV_PFM_GROUP_INFO_VPU1>,
				<SDRV_PFM_GROUP_INFO_ETH1_2>,
				<SDRV_PFM_GROUP_INFO_DMA5>,
				<SDRV_PFM_GROUP_INFO_G2D1>,
				<SDRV_PFM_GROUP_INFO_ALL>;
				status = "disabled";
		};
	};
};
