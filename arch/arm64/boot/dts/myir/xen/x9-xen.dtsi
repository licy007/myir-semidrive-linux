		/* This damc is reserved for safety and change to compatible for differnt driver*/
		dmac0: dma@35500000 {
			compatible = "snps,axi-dma1-1.01a";
			reg = <0 0x35500000 0 0x10000>, /* dma reg address. */
			     <0 0X301e0000 0 0x10000>;  /* dma mux address for mux mode. */
			#dma-cells = <1>;
			clocks = <&HIS_BUS_2>, <&HIS_BUS_2>;//
			clock-names = "core-clk", "cfgr-clk";
			interrupts = <0 0 4>;

			dma-channels = <8>;
			snps,dma-masters = <1>;
			snps,data-width = <3>;
			snps,block-size = <0x10000 0x10000 0x10000 0x10000 0x10000 0x10000 0x10000 0x10000>;
			snps,priority = <0 1 2 3 4 5 6 7>;
			snps,axi-max-burst-len = <4>;
			status = "disabled";
		};
