/ {
	compatible = "semidrive,kunlun";
        interrupt-parent = <&gic>;
        #address-cells = <2>;
	#size-cells = <2>;

	psci: psci {
		compatible	= "arm,psci";
		method		= "smc";
		cpu_on		= <0xc4000003>;
		cpu_off		= <0x84000002>;
		cpu_suspend	= <0xc4000001>;
	};

        gic: interrupt-controller@35431000 {
		compatible = "arm,gic-400";
		reg = <0 0x35431000 0 0x1000>,
		      <0 0x35432000 0 0x2000>;
		#interrupt-cells = <3>;
		interrupt-controller;
		interrupts = <GIC_PPI 9 (GIC_CPU_MASK_SIMPLE(6) | IRQ_TYPE_LEVEL_HIGH)>;
		interrupt-parent = <&gic>;
	};

        soc {
		compatible = "simple-bus";
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		smmu: smmu@35800000 {
			compatible = "arm,smmu-v3";
			reg = <0x0 0x35800000 0x0 0x800000>;
			interrupt-parent = <&gic>;
			interrupts = <0 277 IRQ_TYPE_EDGE_RISING>,
					<0 285 IRQ_TYPE_EDGE_RISING>,
					<0 278 IRQ_TYPE_EDGE_RISING>,
					<0 280 IRQ_TYPE_EDGE_RISING>;
			interrupt-names = "eventq", "priq", "cmdq-sync", "gerror";
			#iommu-cells = <1>;
			status = "disabled";
		};

		uart0: serial@300f0000 {
			compatible = "snps,dw-apb-uart";
			reg = <0 0x300f0000 0 0x100>;
			reg-shift=<2>;
			reg-io-width=<4>;
			interrupts = <0 16 4>;
			clock-frequency=<84000000>;
			status = "disabled";
		};

		uart8: serial@304c0000 {
			compatible = "snps,dw-apb-uart";
			reg = <0 0x304c0000 0 0x100>;
			reg-shift=<2>;
			reg-io-width=<4>;
			interrupts = <0 24 4>;
			clock-frequency=<60000000>;
			clocks = <&UART_SEC0_9>;
			clock-names = "baudclk";
			status = "disabled";
		};

		uart9: serial@304d0000 {
