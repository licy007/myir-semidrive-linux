&i2c7 {
	status = "okay";
	dvp_tp2850:dvp_tp2850@44 {
		compatible = "tp,tp2850";
		reg = <0x44>;
		sync = <0>;
		rst-gpios = <&port4c 19 GPIO_ACTIVE_HIGH>;//index(79) gpio D15
		port {
			cam2_to_parallel:endpoint@0 {
				remote-endpoint = <&csiparallel0_in>;
			};
		};
