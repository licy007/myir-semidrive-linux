&uart15 {
//linux debug
	dmas = <&dmac5 X9_DMA_SLV_ID_UART15_TX>, <&dmac5 X9_DMA_SLV_ID_UART15_RX>;
	dma-names = "tx", "rx";
	status = "okay";
};

&uart1 {
//xm1 --need flip
