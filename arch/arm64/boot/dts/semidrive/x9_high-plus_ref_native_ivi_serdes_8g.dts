					remote-endpoint = <&csimipi1_3_in>;
				};
			};
		};
	};

	max96706:max96706@78 {
		compatible = "sdrv,semidrive_serdes";
		clock-frequency = <24000000>;
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x78>;
		width = <1280>;
		height = <720>;
		ser_addr = <0x40>;
		pwdn-gpios = <&port4c 19 0>;
