			port@2 {
				#address-cells = <1>;
				#size-cells = <0>;
				reg = <2>;
				remap_ser_addr = <0x4a>;
				max96722_to_mipi_2: endpoint@0 {
					remote-endpoint = <&csimipi1_2_in>;
